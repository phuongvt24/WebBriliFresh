@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Đánh giá sản phẩm</title>

    <link rel="stylesheet" href="~/HomeAssets/asset/fonts/themify-icons/themify-icons.css">
    <link rel="stylesheet" href="~/HomeAssets/asset/base.css" />
    <link rel="stylesheet" href="~/HomeAssets/homeheadermain.css" />
    <link rel="stylesheet" href="~/HomeAssets/HomeFooter.css" />
    <link rel="stylesheet" href="~/MyAccountAssets/MyAccount/MyAccountMain.css" />


    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick-theme.min.css" integrity="sha512-17egcferpgzkcm0j0feq1ycjuyawdz9kutv1ejvuaoz8pdnh/0nzxmu6bbxwaaxqoi9pqxnrwqlcdb027hgv9a==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.css" integrity="sha512-yhknp1/awr+yx26cb1y0cjvqumvea2pfzt1c9lls4prq5notzfwbhbig+x9g9eyw/8m0/4oxnx8pxj6z57x0dw==" crossorigin="anonymous" referrerpolicy="no-referrer" />


    <link rel="stylesheet" href="~/MyAccountAssets/ManageOrder/ManageOrder.css" />
    <link rel="stylesheet" href="~/MyAccountAssets/ManageFeedback/ManageFeedback.css" />
    <link rel='stylesheet prefetch' href='https://netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css'>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css">
</head>
<body>
    <!-- Begin header -->
    @await Html.PartialAsync("~/Views/Shared/_HomeHeaderPartialView_login.cshtml")
    <!-- End header -->
    <!-- Begin content -->
    <div class="container">
        <div class="row">
            <section class="directory mt-3">
                <nav class="navbar navbar-expand-xl navbar-light ">
                    <div class="container-fluid">
                        <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Trang chủ</a></li>
                                <li class="breadcrumb-item active" id="ManageFeedback" aria-current="page">Đánh giá sản phẩm</li>
                            </ol>
                        </nav>
                    </div>
                </nav>
            </section>

            @await Html.PartialAsync("~/Views/Shared/_SubNavPartialView.cshtml")
            <!-- End SlideBar -->

            <div class="col-12 col-xl-9 container-right">
                <div class="right-content">
                    <div id="subnav">
                        <div class="heading-right">Đánh giá sản phẩm</div>
                        <ul class="subnav-order">
                            <li><a class="active" href="#">Chưa đánh giá</a></li>
                            <li><a href="#">Lịch sử đánh giá</a></li>
                        </ul>
                    </div>

                    <div class="sucess-order order-info mt-3">
                        <div class="transfer-info">
                            <p>Đơn hàng <span id="order-id">380373322120636</span></p>
                            <p>Đặt ngày <span id="order-date">11/11/2022 02:01:34</span></p>
                            <p>Kiện hàng của bạn đã được giao thành công vào ngày <span id="shipping-date">13/11/2022 12:38:45</span></p>
                        </div>
                        <div class="header-order">
                            <span><i class="ti-home"></i> JFruit</span>
                            <span><i class="ti-truck"></i> Đã giao</span>
                        </div>
                        <div class="content-order">
                            <div class="img-product"><img src="./assets/image/item_2.jpg" alt=""></div>
                            <div class="product-info">
                                <p class="name">Dưa hấu ruột đỏ Long An</p>
                                <p style="color: #757575;">Đơn vị: <span class="mass">1kg</span></p>
                            </div>
                            <div class="purchase-info unit-price">Đơn giá: <span>35,000</span></div>
                            <div class="purchase-info quantity">Số lượng: <span>1</span></div>
                            <div class="purchase-info amount">Thành tiền: <span>35,000</span></div>
                            <div class="order-rating-btn"><a class="btn-shared js-rating-btn">Đánh giá</a></div>
                        </div>
                        <div class="content-order">
                            <div class="img-product"><img src="./assets/image/item_3.jpg" alt=""></div>
                            <div class="product-info">
                                <p class="name">Cá hồi tươi Fillet Na Uy</p>
                                <p style="color: #757575;">Đơn vị: <span class="mass">200g</span></p>
                            </div>
                            <div class="purchase-info unit-price">Đơn giá: <span>128,000</span></div>
                            <div class="purchase-info quantity">Số lượng: <span>1</span></div>
                            <div class="purchase-info amount">Thành tiền: <span>128,000</span></div>
                            <div class="order-rating-btn"><a class="btn-shared js-rating-btn">Đánh giá</a></div>
                        </div>
                        <p>Tổng tiền: <b><span class="total-order">163,000</span></b></p>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- Begin Modal Rating Product-->
    <div class="modal js-modal">
        <form action="" class="modal-container js-modal-container">
            <div class="modal-close js-modal-close">
                <i class="ti-close"></i>
            </div>

            <header class="modal-header">
                Đánh giá sản phẩm
            </header>

            <div class="modal-body">
                <p id="modal-label-checkbox" for="address-default">Chất lượng sản phẩm (1 sao: Rất Tệ - 5 sao: Rất Tốt)</p>
                <div class="stars">
                    <input class="star star-5" id="star-5" type="radio" name="star" />
                    <label class="star star-5" for="star-5"></label>
                    <input class="star star-4" id="star-4" type="radio" name="star" />
                    <label class="star star-4" for="star-4"></label>
                    <input class="star star-3" id="star-3" type="radio" name="star" />
                    <label class="star star-3" for="star-3"></label>
                    <input class="star star-2" id="star-2" type="radio" name="star" />
                    <label class="star star-2" for="star-2"></label>
                    <input class="star star-1" id="star-1" type="radio" name="star" />
                    <label class="star star-1" for="star-1"></label>
                </div>

                <label class="modal-label" for="flavor">Hương vị/ Màu sắc</label>
                <input id="flavor" class="modal-input" type="text" placeholder="Tươi ngon">

                <label class="modal-label" for="package">Bao bì, mẫu mã</label>
                <input id="package" class="modal-input" type="text" placeholder="Chắc chắn">

                <label class="modal-label" for="description">Mô tả</label>
                <textarea name="" id="description" cols="30" rows="10"></textarea>

                <label class="modal-image btn-shared">
                    <i class="ti-camera"></i>
                    <span>Thêm hình ảnh</span>
                    <input type="file" accept="image/*">
                </label>
            </div>

            <footer class="modal-footer">
                <input id="rate-btn" class="btn-modal" type="submit" value="Hoàn tất">
            </footer>
        </form>
    </div>
    @await Html.PartialAsync("~/Views/Shared/_FooterPartialView.cshtml")
    <script type="text/javascript" src="~/MyAccountAssets/MyAccount/SubNavSide.js"></script>
    <script src="~/MyAccountAssets/ManageFeedback/ManageFeedback.js"></script>
</body>
</html>